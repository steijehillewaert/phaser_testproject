{
  "name": "phaser_testproject",
  "version": "1.0.0",
  "description": "Test project with webpack and phaser",
  "main": "index.js",
  "repository": "https://github.com/steijehillewaert/phaser_testproject.git",
  "author": "Steije Hillewaert <steije.hillewaert@student.howest.be>",
  "license": "MIT",
  "scripts": {
    "development": "webpack-dev-server --open 'Google Chrome'",
    "production": "npm run clean && webpack && npm run copy",
    "copy": "copyfiles -u 1 ./src/index.html ./src/js/phaser.min.js ./src/assets/**/*.* ./dist",
    "clean": "rimraf dist",
    "serve": "http-server ./dist -p 8080"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "copyfiles": "^1.0.0",
    "http-server": "^0.9.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
